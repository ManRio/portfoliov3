{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "manrio.dev" }],
      "destination": "https://www.manrio.dev/:path*",
      "permanent": true
    },
    { "source": "/index.html", "destination": "/", "permanent": true },
    { "source": "/:path*/index.html", "destination": "/:path*", "permanent": true }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
